{
  "expo": {
    "name": "HealthyApps",
    "slug": "HealthyApps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "INTERNET"
      ],
      "package": "com.skripsi.healthyapps"
    },
    "extra": {
      "eas": {
        "projectId": "71c5ea9a-2d30-45d8-82c2-59dd48e78a78"
      }
    }
  }
}
